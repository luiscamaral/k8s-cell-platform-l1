# Example: Vault Token Secret
# Copy to vault-token-secret.yaml and fill in your token
# This file is gitignored - DO NOT commit with real token
apiVersion: v1
kind: Secret
metadata:
  name: vault-token
  namespace: external-secrets
type: Opaque
stringData:
  # Get token from: vault token create -policy=kubernetes-secrets
  token: "hvs.XXXXXXXXXXXXXXXXXXXXXXXXXX"
